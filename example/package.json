{
  "name": "webpack-typescript-starter",
  "version": "0.0.1",
  "description": "A simple Webpack 4 starter with TypeScript transpilation",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server --host 0.0.0.0 --port 9000 --inline --progress --profile --colors --watch --content-base ./src/ --mode development",
    "build": "webpack --config webpack.config.js --mode production",
    "build.prod": "webpack --config webpack.config.js -p",
    "test": "karma start karma.config.js",
    "kill": "kill -9 $(lsof -t -i:9000 -sTCP:LISTEN)"
  },
  "keywords": [
    "webpack",
    "webpack4",
    "typescript"
  ],
  "author": "Juri Strumpflohner",
  "license": "ISC",
  "devDependencies": {
    "@types/animejs": "^2.0.1",
    "@types/jasmine": "2.8.7",
    "@types/lodash": "^4.14.120",
    "@types/node": "7.0.0",
    "@types/vue": "^2.0.0",
    "css-loader": "^2.1.0",
    "csstype": "^2.6.2",
    "jasmine-core": "3.1.0",
    "karma": "2.0.4",
    "karma-jasmine": "1.1.2",
    "karma-phantomjs-launcher": "1.0.4",
    "karma-webpack": "3.0.0",
    "less-loader": "^4.1.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "source-map-loader": "0.2.3",
    "svg-url-loader": "^2.3.2",
    "ts-loader": "^5.3.3",
    "tslint": "5.10.0",
    "tslint-loader": "3.6.0",
    "typescript": "^3.3.3333",
    "vue-class-component": "^6.3.2",
    "vue-contenteditable-directive": "^1.2.0",
    "vue-drag-drop": "^1.1.4",
    "vue-json-editor": "^1.2.3",
    "vue-loader": "^15.6.1",
    "vue-property-decorator": "^7.3.0",
    "vue-router": "^3.0.2",
    "vue-schema-based-json-editor": "^7.20.2",
    "vue-slider-component": "^2.8.6",
    "vue-template-compiler": "^2.5.22",
    "wait-until": "0.0.2",
    "webpack": "4.12.0",
    "webpack-cli": "3.0.8",
    "webpack-dev-server": "3.1.4"
  },
  "dependencies": {
    "animejs": "^3.0.1",
    "bootstrap-vue": "^2.0.0-rc.11",
    "eventemitter3": "^3.1.0",
    "interactjs": "^1.3.4",
    "js-file-download": "^0.4.4",
    "lodash": "^4.17.11",
    "mime-types": "^2.1.22",
    "moment": "^2.24.0",
    "parsehtml": "^1.0.0",
    "path-parse": "^1.0.6",
    "rulez.js": "^0.3.1",
    "strapi-sdk-javascript": "^0.3.1",
    "vue": "^2.5.22",
    "vue-perfect-scrollbar": "^0.1.0",
    "vue-sidebar-menu": "^3.4.0",
    "vuex": "^3.1.0"
  }
}
